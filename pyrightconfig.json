{
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/*.js",
    "**/*.css",
    "**/*.html",
    "**/*.json",
    "**/*.md",
    "**/*.txt",
    "**/*.xml",
    "**/*.svg",
    "**/*.jpg",
    "**/*.png",
    "**/*.ico",
    "**/*.toml",

    ".babelrc",
    "**/.babelrc",
    ".eslintrc.js",
    "**/.eslintrc.js",
    ".stylelintrc.json",
    "**/.stylelintrc.json",
    ".htaccess",
    "**/.htaccess",
    "webpack.config.js",
    "**/webpack.config.js",
    "postcss.config.js",
    "**/postcss.config.js",
    "lighthouserc.js",
    "**/lighthouserc.js",
    "netlify.toml",
    "**/netlify.toml",
    "vercel.json",
    "**/vercel.json",
    "manifest.json",
    "**/manifest.json",
    "package.json",
    "**/package.json",
    "robots.txt",
    "**/robots.txt",
    "sitemap.xml",
    "**/sitemap.xml",
    "service-worker.js",
    "**/service-worker.js",
    "server.ps1",
    "**/server.ps1",
    "node_modules/**",
    "dist/**",
    "build/**",
    ".vercel/**",
    "assets/**"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.8",
  "typeCheckingMode": "basic"
}